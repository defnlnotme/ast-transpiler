{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "indent": ["error", 4, { "SwitchCase": 0 }],
      "no-trailing-spaces": "error",
      "no-console": 1,
      "semi": "error"
    }
  }
